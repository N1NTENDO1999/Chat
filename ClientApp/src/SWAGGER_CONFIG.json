{
    "$schema": "../node_modules/ng-swagger-gen/ng-swagger-gen-schema.json",
    "swagger": "http://localhost:55056/swagger/v1/swagger.json",
    "output": "src/app/core/api",
    "input": "http://localhost:55056/swagger/v1/swagger.json",
    "prefix": "Api",
    "ignoreUnusedModels": true,
    "minParamsForContainer": 2,
    "sortParams": "desc",
    "defaultTag": "Api",
    "removeStaleFiles": true,
    "modelIndex": true,
    "serviceIndex": true,
    "apiModule": true,
    "enumModule": true,
    "generateExamples": false
  }